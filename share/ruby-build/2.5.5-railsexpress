build_package_patch_ruby_railsexpress() {
  fetch_git rvm-patchsets git://github.com/skaes/rvm-patchsets.git master
  cat rvm-patchsets/patches/ruby/2.5.5/railsexpress/* | patch -p1
}

install_package "openssl-1.1.1g" "https://www.openssl.org/source/openssl-1.1.1g.tar.gz#ddb04774f1e32f0c49751e21b67216ac87852ceb056b75209af2443400636d46" mac_openssl --if has_broken_mac_openssl
install_package "ruby-2.5.5" "https://cache.ruby-lang.org/pub/ruby/2.5/ruby-2.5.5.tar.bz2#1f2567a55dad6e50911ce42fcc705cf686924b897f597cabf803d88192024dcb" patch_ruby_railsexpress warn_unsupported ldflags_dirs enable_shared standard verify_openssl
